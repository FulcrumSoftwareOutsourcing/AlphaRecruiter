  <entity id="Years70"
          entity_class_id="BASE_ENTITY"
          cached="true"
          in_security="false"
          >
    <sql_select>
      <![CDATA[
        with CurrentYear as
          (
            select YEAR(getdate()) as y
          )
        select CurrentYear.y - n + 1 as year
          from f_GetNumbersTable(1, 70),
               CurrentYear 
      ]]>
    </sql_select>
    <attributes>
      <attribute id="Year"
                 type="int"
                 primary_key="true"
      />
    </attributes>
  </entity>
