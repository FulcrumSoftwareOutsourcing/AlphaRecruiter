                 type="int"
                 grid_width="120"
      />
      <attribute id="Saturday"
                 caption="Saturday"
                 type="int"
                 grid_width="120"
      />
      <attribute id="Sunday"
                 caption="Sunday"
                 type="int"
                 grid_width="120"
      />
      <attribute id="WeekTotal"
                 caption="&lt;B&gt;Month Total&lt;/B&gt;"
                 type="int"
                 grid_width="120"
