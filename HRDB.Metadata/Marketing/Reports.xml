  <!--                       Applicants by Years                           -->
  <!-- -*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*- -->
  <entity id="Reports_Applicants_by_Years"
          single_caption="Applicants by Years"
          plural_caption="Applicants by Years"
          filter_enabled="false"
          paging_enabled="false"
          display_all_records_without_footer="true"
          entity_class_id="BASE_ENTITY">
    <sql_select>
      <![CDATA[
          with PivotTable
                    as
                    (
                      SELECT m AS [Month], [2011], [2012], [2013], [2014], [2015], [2016]
                          ,isnull([2011], 0) + isnull([2012], 0) + isnull([2013], 0) + 
                           isnull([2014], 0) + isnull([2015], 0) + isnull([2016], 0) as [YearTotal]
                        FROM
                        (
                        select year(ap.CreationDate) y, month(ap.CreationDate) m, count(*) cnt
                          from HR_Applicant ap
                         group by year(ap.CreationDate), month(ap.CreationDate)
                        ) AS SourceTable
                        PIVOT
                        (
                        sum(cnt)
                        FOR y IN ([2011], [2012], [2013], [2014], [2015], [2016])
                        ) AS PivotT
                  )
                  select PivotTable.*
                    from PivotTable
                   union all
                  select 0 as Month, 
                         sum([2011]) as [2011], 
                         sum([2012]) as [2012], 
                         sum([2013]) as [2013], 
                         sum([2014]) as [2014], 
                         sum([2015]) as [2015], 
                         sum([2016]) as [2016],
                         Sum(YearTotal) as [YearTotal]
                    from PivotTable      ]]>
    </sql_select>
    <attributes>
      <attribute id="Month"
                 caption="Month"
                 type="string"
                 primary_key="true"
                 row_source_id="Months_Name_with_Total_Lookup"
                 grid_width="120"
      />
      <attribute id="2011"
                 caption="2011"
                 type="int"
                 grid_width="120"
      />
      <attribute id="2012"
                 caption="2012"
                 type="int"
                 grid_width="120"
      />
      <attribute id="2013"
                 caption="2013"
                 type="int"
                 grid_width="120"
      />
      <attribute id="2014"
                 caption="2014"
                 type="int"
                 grid_width="120"
      />
      <attribute id="2015"
                 caption="2015"
                 type="int"
                 grid_width="120"
      />
      <attribute id="2016"
                 caption="2016"
                 type="int"
                 grid_width="120"
      />
      <attribute id="YearTotal"
                 caption="&lt;B&gt;Years Total&lt;/B&gt;"
                 type="int"
                 grid_width="120"
      />
    </attributes>
  </entity>
