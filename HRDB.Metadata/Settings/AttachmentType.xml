                entity_id="HR_AttachmentType">
  </entity_usage>
  <!-- -*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*- -->
  <row_source id="HR_AttachmentType_Lookup"
              entity_usage_id="HR_AttachmentType_ForLookUp"
